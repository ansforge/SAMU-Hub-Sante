    },
    {
      "name": "dispatch.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "dead-lettering",
      "pattern": "^.*\\.(?:ack|info|message)$",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 10000,
        "dead-letter-exchange": "distribution.dlx"
      },
      "priority": 0
    }
  ],
  "bindings": [
    {
      "source": "hubsante",
      "vhost": "/",
      "destination": "dispatch",
      "destination_type": "queue",
      "routing_key": "#",
      "arguments": {}
