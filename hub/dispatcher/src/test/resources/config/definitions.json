{
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "exchanges": [
    {
      "name": "hubsante",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "distribution",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
