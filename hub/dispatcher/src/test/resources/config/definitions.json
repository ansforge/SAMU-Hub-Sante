    {
      "user": "fr.health.samuA",
      "vhost": "/",
      "configure": "fr.health.samuA.*",
      "write": "hubsante",
      "read": "fr.health.samuA.*"
    },
    {
      "user": "fr.health.samuB",
      "vhost": "/",
      "configure": "fr.health.samuB.*",
      "write": "hubsante",
      "read": "fr.health.samuB.*"
    },
    {
      "user": "fr.fire.nexsis.sdisZ",
      "vhost": "/",
      "configure": "fr.fire.nexsis.sdisZ.*",
