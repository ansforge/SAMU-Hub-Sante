<?xml version="1.0" encoding="UTF-8"?>
<jxb:bindings xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
              jxb:version="2.1"
              xmlns:xs="http://www.w3.org/2001/XMLSchema"
              xmlns:inheritance="http://jaxb2-commons.dev.java.net/basic/inheritance"
              xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
              jxb:extensionBindingPrefixes="xjc"
>
    <jxb:bindings schemaLocation="../xsd/cisu.xsd"
                  node="/xs:schema/xs:element[@name='ackEvent']/xs:complexType">
        <inheritance:implements>com.hubsante.model.Event</inheritance:implements>
    </jxb:bindings>
    <jxb:bindings schemaLocation="../xsd/cisu.xsd"
                  node="/xs:schema/xs:element[@name='createEvent']/xs:complexType">
        <inheritance:implements>com.hubsante.model.Event</inheritance:implements>
    </jxb:bindings>
    <jxb:bindings schemaLocation="../xsd/cisu.xsd"
                  node="/xs:schema/xs:element[@name='updateEvent']/xs:complexType">
        <inheritance:implements>com.hubsante.model.Event</inheritance:implements>
    </jxb:bindings>
    <jxb:bindings schemaLocation="../xsd/cisu.xsd"
                  node="/xs:schema/xs:element[@name='cancelEvent']/xs:complexType">
        <inheritance:implements>com.hubsante.model.Event</inheritance:implements>
    </jxb:bindings>
<!--    <jxb:bindings schemaLocation="../xsd/cisu.xsd"-->
<!--                  node="/xs:schema/xs:element[@name='ackMessage']">-->
<!--        <inheritance:implements>com.hubsante.model.Event</inheritance:implements>-->
<!--    </jxb:bindings>-->
    <jxb:bindings schemaLocation="../xsd/message.xsd">
        <jxb:bindings
                node="/xs:schema/xs:element[@name='ackMessageId']">
            <jxb:class name="AckMessageId"/>
        </jxb:bindings>
    </jxb:bindings>
    <jxb:bindings schemaLocation="../xsd/message.xsd">
        <jxb:bindings
                node="/xs:schema/xs:complexType[@name='ackMessageId']">
            <jxb:class name="AckMessageIdType"/>
        </jxb:bindings>
    </jxb:bindings>
    <jxb:bindings schemaLocation="../xsd/cisu.xsd"
                  node="/xs:schema/xs:element[@name='message']/xs:complexType/xs:sequence/xs:choice">
        <jxb:property name="event"/>
    </jxb:bindings>
    <jxb:globalBindings choiceContentProperty="true"/>
</jxb:bindings>