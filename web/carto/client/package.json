{
  "name": "dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "cd ../../.. && husky web/carto/client/.husky",
    "setup": "npm run cp:schemas && npm run cp:examples",
    "dev": "nuxt dev",
    "vite": "vite",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider nuxi build --force",
    "start": "nuxt start",
    "generate": "cross-env NODE_OPTIONS=--openssl-legacy-provider nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "lint": "npm run lint:js && npm run lint:css",
    "lintfix": "npm run lint:js -- --fix && npm run lint:css -- --fix"
  },
  "dependencies": {
    "@babel/types": "^7.24.7",
    "@koumoul/vjsf": "^3.0.0-beta.43",
    "@koumoul/vjsf-markdown": "^0.2.1",
    "@mdi/font": "^7.4.47",
    "@pinia/nuxt": "^0.5.1",
    "@types/node": "^20.14.9",
    "ajv-formats": "^2.1.1",
    "consola": "^3.2.3",
    "core-js": "^3.37.1",
    "eslint-plugin-vuetify": "^2.4.0",
    "leaflet": "^1.9.4",
    "leaflet.icon.glyph": "^0.3.0",
    "markdown-it": "^13.0.2",
    "moment": "^2.30.1",
    "nuxt": "^3.12.2",
    "pinia": "^2.1.7",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "uuid": "^9.0.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.30",
    "vue-json-viewer": "^3.0.4",
    "vue-server-renderer": "^2.7.16",
    "vue-template-compiler": "^2.7.16",
    "vue3-toastify": "^0.2.3",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.7",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.15.0",
    "cypress-cdp": "^1.5.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.2",
    "nuxi": "^3.12.0",
    "prettier": "^2.8.8",
    "prettier-plugin-vue": "^1.1.6",
    "sass": "^1.77.8",
    "stylelint": "^16.13.2",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-scss": "^6.10.1",
    "typescript": "^5.5.3",
    "vuetify": "^3.6.10"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}