{
  "name": "dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "setup": "npm run cp:schemas && npm run cp:examples",
    "cp:schemas": "rm static/schemas/json-schema/* && cp -R ../../../../SAMU-Hub-Modeles/src/main/resources/json-schema/ static/schemas/",
    "cp:examples": "rm static/examples/* && cp -R ../../../../SAMU-Hub-Modeles/src/main/resources/sample/examples/ static/",
    "dev": "nuxt dev",
    "vite": "vite",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider nuxi build --force",
    "start": "nuxt start",
    "generate": "cross-env NODE_OPTIONS=--openssl-legacy-provider nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "lintfix": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "@babel/types": "^7.24.7",
    "@koumoul/vjsf": "^3.0.0-beta.43",
    "@koumoul/vjsf-markdown": "^0.2.1",
    "@mdi/font": "^7.4.47",
    "@nuxtjs/dotenv": "^1.4.2",
    "@pinia/nuxt": "^0.5.1",
    "@types/node": "^20.14.9",
    "ajv-formats": "^2.1.1",
    "consola": "^3.2.3",
    "core-js": "^3.37.1",
    "eslint-plugin-vuetify": "^2.4.0",
    "jsonpath": "^1.1.1",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "markdown-it": "^13.0.2",
    "moment": "^2.30.1",
    "nuxt": "^3.12.2",
    "pinia": "^2.1.7",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "uuid": "^9.0.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.30",
    "vue-json-viewer": "^3.0.4",
    "vue-server-renderer": "^2.7.16",
    "vue-template-compiler": "^2.7.16",
    "vue3-toastify": "^0.2.3",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.7",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.15.0",
    "cypress-cdp": "^1.5.3",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.5.1",
    "nuxi": "^3.12.0",
    "sass": "^1.77.8",
    "typescript": "^5.5.3",
    "vuetify": "^3.6.10"
  }
}
